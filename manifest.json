{
    "manifest_version": 2,
    "name": "YTAutoResume",
    "version": "0.1",
    "description": "DEEGUS",
    "short_name": "DEEEGUS",
    "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],

    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js": [
                "jquery-3.5.1.min.js",
                "content.js"
            ]
        }
    ],

    "browser_action": {
      "default_title": "Does a thing when you do a thing",
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon128.png"
      }
    }
   }